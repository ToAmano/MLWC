&CONTROL
  calculation = 'cp-wf'
  dt =   5.0
!  ekin_conv_thr =   1.0000000000d-07 
!  etot_conv_thr =   1.0000000000d-09 
  iprint = 10
  isave = 100
! different from 1  
  ndr = 55
  ndw = 55
! =================
  nstep = 10000
  outdir = './out/'
  prefix = 'aiida'
  pseudo_dir = './pseudo/'
  restart_mode = 'reset_counters' ! defferent from 1
  tprnfor = .true.
  tstress = .true.
  verbosity = 'high'
  max_seconds = 64800
!  wf_collect = .true.
/
&SYSTEM
!  ecutrho =   400
  ecutwfc =   100
  ibrav = 0
  nat  = 64
  ntyp = 2
  nr1b = 20
  nr2b = 20
  nr3b = 20
/
&ELECTRONS
  conv_thr =   1.0000000000d-08
  electron_damping =   0.05
!   electron_dynamics = 'damp'
  electron_dynamics = 'verlet' ! md
  electron_velocities = 'zero'
  emass     =   4.0000000000d+02
  ortho_eps =   1.0000000000d-11
  orthogonalization = 'ortho'
/
&IONS
  ! ion_dynamics = 'none'
  ! add two lines for random displacements
  ! tranp = .true.       ! randomize atomic positions
  ! amprp = 0.03         ! displacement amplitude for the randomization/
  ion_dynamics = 'verlet'
  ion_velocities = 'zero'
  ion_temperature = 'nose', fnosep = 25.0, tempw = 40.0
/

&WANNIER
   calwf=3   !  #Used with calwf=3, contains the wannier centers along a trajectory.
   adapt=.true.
   nsteps=20
   nit=60    ! Number of iterations to do for Wannier convergence.
   !tolw=1.e-6
   wfdt=4.e0 ! The minimum step size to take in the SD/CG direction (default 5.D0)
   wf_q= 500
   wf_friction=0.3e0 !# Damping coefficient for Damped-Dynamics. (default 0.3D0)
/
ATOMIC_SPECIES
